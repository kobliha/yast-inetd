{
    SCR::RegisterAgent(.test.inetd, `ag_netd (`Xinetd (WFM::Args (0))));

    list s = (list) SCR::Read (.test.inetd.services);
    map ns = $[
	// no iid
	"changed": true,
	"comment": " Dys is mai servis",
	"enabled": false,
	"service": "sgi_fam",
	"rpc_version": "1-2",
	"socket_type": "stream",
	"protocol": "tcp",
	"wait": false,
	"max": 42, //ignored
	"user": "root",
	"group": "bin",
	"server": "/usr/sbin/fam",
	"server_args": "fam",
	];
    s = add (s, ns);
    boolean r = (boolean) SCR::Write (.test.inetd.services, s);

    return r;
}
